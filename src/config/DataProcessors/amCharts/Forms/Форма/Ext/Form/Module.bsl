&НаКлиенте
Функция AmChartsВременныйКаталогКартинок()
	#Если ВебКлиент Тогда
	__вркат = "e1cib/tempstorage/";	
	#Иначе
	__вркат = КаталогВременныхФайлов();
	AmChartsМакетВФайл("бсDragIcon",__вркат+"dragIcon.gif");
	AmChartsМакетВФайл("бсLens",__вркат+"lens.png");
	AmChartsМакетВФайл("бсDragIconH",__вркат+"dragIconH.gif");
	AmChartsМакетВФайл("бсДлительнаяОперация",__вркат+"ДлительнаяОперация.gif");
	__вркат = "file://"+СтрЗаменить(__вркат,"\","/");
	#КонецЕсли
	Возврат __вркат;

КонецФункции // AmChartsВременныйКаталогКартинок()

&НаКлиенте
Процедура AmChartsМакетВФайл(имямак,имяф)

	AmChartsПолучитьМакет(имямак).Записать(имяф);

КонецПроцедуры // AmChartsМакетВФайл()

&НаСервере
Функция AmChartsПолучитьМакет(имямак)

	об = РеквизитФормыВЗначение("Объект");
	Возврат об.ПолучитьМакетОбраб(имямак);

КонецФункции // ()

