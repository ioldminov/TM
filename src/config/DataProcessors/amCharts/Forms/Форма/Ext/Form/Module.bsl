	стр = "";
	Для каждого пар Из Основа.Параметры Цикл
		пзнач = Неопределено;
		Если пар.Значение = Неопределено Тогда
			Продолжить;
		ИначеЕсли ТипЗнч(пар.Значение)=Тип("Число") Тогда
			пзнач = Формат(пар.Значение,"ЧРД=.; ЧН=0; ЧГ=0");
		ИначеЕсли ТипЗнч(пар.Значение)=Тип("Булево") Тогда
			пзнач = ?(пар.Значение,"true","false");
		Иначе
			Если Лев(пар.Значение,2)="##" Тогда //значение - это имя переменной
				пзнач = СокрЛП(Сред(пар.Значение,3));
			Иначе
				пзнач = """"+СокрЛП(пар.Значение)+"""";
			КонецЕсли; 
		КонецЕсли; 
		стр = стр+"chart."+пар.Ключ+"="+пзнач+";"+Символы.ПС;
	КонецЦикла; 
	Для каждого об Из Основа.Объекты Цикл
		стр = стр+"var "+об.Имя+" = new AmCharts."+об.Тип+"();"+Символы.ПС;
		Для каждого пар Из об.Параметры Цикл
			пзнач = Неопределено;
