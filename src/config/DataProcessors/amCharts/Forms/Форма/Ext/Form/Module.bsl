Функция AmChartsПолучитьHTMLТекст(Основа,ОбъектСМакетами)

	встр = ОбъектСМакетами.ПолучитьМакетОбраб("бсМакетДиаграммыJS").ПолучитьТекст();
	
	встр = СтрЗаменить(встр, "##amcharts.js##", ОбъектСМакетами.ПолучитьМакетОбраб("бсAmchartsJS").ПолучитьТекст());
	встр = СтрЗаменить(встр, "##serial.js##", ?(Основа.ТипЧарта="AmSerialChart",ОбъектСМакетами.ПолучитьМакетОбраб("бсSerialJS").ПолучитьТекст(),""));
	встр = СтрЗаменить(встр, "##xy.js##", ?(Основа.ТипЧарта="AmXYChart",ОбъектСМакетами.ПолучитьМакетОбраб("бсXYJS").ПолучитьТекст(),""));
	встр = СтрЗаменить(встр, "##funnel.js##", ?(Основа.ТипЧарта="AmFunnelChart",ОбъектСМакетами.ПолучитьМакетОбраб("бсFunnelJS").ПолучитьТекст(),""));
	встр = СтрЗаменить(встр, "##gauge.js##", ?(Основа.ТипЧарта="AmAngularGauge",ОбъектСМакетами.ПолучитьМакетОбраб("бсGaugeJS").ПолучитьТекст(),""));
	встр = СтрЗаменить(встр, "##pie.js##", ?(Основа.ТипЧарта="AmPieChart",ОбъектСМакетами.ПолучитьМакетОбраб("бсPieJS").ПолучитьТекст(),""));
	встр = СтрЗаменить(встр, "##radar.js##", ?(Основа.ТипЧарта="AmRadarChart",ОбъектСМакетами.ПолучитьМакетОбраб("бсRadarJS").ПолучитьТекст(),""));
	
	встр = СтрЗаменить(встр, "##ТелоФункции##", Основа.ТелоФункции);
	встр = СтрЗаменить(встр, "##Данные##", Основа.Данные);
	Если ЗначениеЗаполнено(Основа.КолонкаРасшифровки) Тогда
		встр = СтрЗаменить(встр,"//outResultBuffer = event.item.dataContext.##КолонкаРасшифровки##;","outResultBuffer = event.item.dataContext."+Основа.КолонкаРасшифровки+";");
	Иначе
		встр = СтрЗаменить(встр,"chart.addListener(""clickGraphItem"", handleClick); //##click listener##","");
	КонецЕсли; 
	встр = СтрЗаменить(встр, "##ИмяЧарта##", Основа.ТипЧарта);
	
	#Если ВебКлиент Тогда
	врхр = ПоместитьВоВременноеХранилище(ОбъектСМакетами.ПолучитьМакетОбраб("бсDragIcon"));
	встр = СтрЗаменить(встр, "dragIcon.gif", СтрЗаменить(врхр, "e1cib/tempstorage/", ""));
	врхр = ПоместитьВоВременноеХранилище(ОбъектСМакетами.ПолучитьМакетОбраб("бсLens"));
	встр = СтрЗаменить(встр, "lens.png", СтрЗаменить(врхр, "e1cib/tempstorage/", ""));
	врхр = ПоместитьВоВременноеХранилище(ОбъектСМакетами.ПолучитьМакетОбраб("бсDragIconH"));
	встр = СтрЗаменить(встр, "dragIconH.gif", СтрЗаменить(врхр, "e1cib/tempstorage/", ""));
	врхр = ПоместитьВоВременноеХранилище(ОбъектСМакетами.ПолучитьМакетОбраб("бсДлительнаяОперация"));
	встр = СтрЗаменить(встр, "ДлительнаяОперация.gif", врхр);   	
