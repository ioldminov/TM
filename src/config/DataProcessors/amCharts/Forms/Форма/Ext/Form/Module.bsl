Функция AmChartsДанные(тз)

	встр = "";
	Для каждого стр Из тз Цикл
		стрд = "";
		Для каждого кол Из тз.Колонки Цикл
			Если НЕ (стр[кол.Имя]=Неопределено) Тогда
				Если ТипЗнч(стр[кол.Имя])=Тип("Число") Тогда
					стрд = стрд + ?(стрд="","",", ")+
								"""" + кол.Имя + """: " + Формат(стр[кол.Имя],"ЧРД=.; ЧН=; ЧГ=0") +Символы.ПС;
				Иначе	
					стрд = стрд + ?(стрд="","",", ")+
								"""" + кол.Имя + """: """ + Формат(стр[кол.Имя],"ЧРД=.; ЧН=; ЧГ=0; ДФ=MM/dd/yyyy") + """"+Символы.ПС;
				КонецЕсли; 
			КонецЕсли; 
		КонецЦикла;
		встр = встр + ?(встр="","{",", {")+стрд+"}"+Символы.ПС;
	КонецЦикла; 
	
	Возврат встр;

